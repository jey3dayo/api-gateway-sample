{
  "name": "api-gateway-sample",
  "version": "1.0.0",
  "description": "reserve system of abema studio",
  "scripts": {
    "test": "npm-run-all -p eslint jest",
    "jest": "jest .",
    "eslint": "eslint --fix --no-eslintrc -c .eslintrc.js .",
    "deploy:env": "sls deploy --stage $NODE_ENV",
    "deploy": "export NODE_ENV=development && yarn deploy:env",
    "deploy:production": "export NODE_ENV=production && yarn deploy:env",
    "deploy:staging": "export NODE_ENV=staging && yarn deploy:env",
    "serve": "sls offline -P 4000",
    "serve:watch": "nodemon -e js,ts,jsx,tsx -x yarn serve",
    "tail:dev": "sls logs --function notifyEvent --tail",
    "test:watch": "jest --watch",
    "build": "sls webpack",
    "watch:dev": "sls invoke local --watch --function notifyEvent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jey3dayo/api-gateway-sample"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "7.15.8",
    "@unly/serverless-env-copy-plugin": "^1.0.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "jest": "^27.2.5",
    "jest-chain": "^1.1.5",
    "jest-extended": "^1.0.0",
    "nodemon": "^2.0.13",
    "npm-run-all": "4.1.5",
    "prettier": "^2.4.1",
    "serverless": "^2.62.0",
    "serverless-dotenv-plugin": "^3.10.0",
    "serverless-layers": "^2.5.1",
    "serverless-offline": "^8.2.0",
    "serverless-plugin-resource-tagging": "^1.1.1",
    "serverless-plugin-warmup": "^6.0.0",
    "serverless-prune-plugin": "^1.6.1",
    "serverless-tag-api-gateway": "^1.0.0",
    "serverless-webpack": "^5.5.4",
    "webpack": "^5.58.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node-fetch": "^3.0.0",
    "objection": "^2.2.17",
    "prop-types": "^15.7.2",
    "url-parse": "^1.5.3",
    "xss": "^1.0.10"
  },
  "private": true
}
